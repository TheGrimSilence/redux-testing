{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "target": "ES2016",
    "sourceMap": false,
    "experimentalDecorators": true,
    "declaration": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "outDir": "../out",
    "typeRoots": ["typings"]
  }
}
